       IDENTIFICATION DIVISION.
       PROGRAM-ID. RVS001.
       AUTHOR. SID WAITE.
       DATE-COMPILED. TODAY.
       ENVIRONMENT DIVISION.
       INPUT-OUTPUT SECTION.
       FILE-CONTROL.
           SELECT RVUFILE ASSIGN TO "S30" ORGANIZATION IS INDEXED
           ACCESS MODE IS DYNAMIC RECORD KEY IS RVU-KEY.
           SELECT FILEIN ASSIGN TO "S35"
           ORGANIZATION LINE SEQUENTIAL.

       DATA DIVISION.
       FILE SECTION.
       FD  FILEIN.
       01  FILEIN01.
           02 FI-CPT PIC X(7).
           02 FI-AMT PIC 9(5)V99.
       FD RVUFILE.
       01 RVUFILE01.
           02 RVU-KEY PIC X(7).
           02 RVU-AMT PIC 9(3)V9999.

       WORKING-STORAGE SECTION.
       PROCEDURE DIVISION.
       P0.
           OPEN OUTPUT RVUFILE.
           CLOSE RVUFILE.
           OPEN I-O RVUFILE.
           OPEN INPUT FILEIN.
       P1.
           READ FILEIN AT END GO TO P2.
           MOVE FI-CPT TO RVU-KEY
           COMPUTE RVU-AMT = FI-AMT / 35.8043
           WRITE RVUFILE01
           GO TO P1.
       P2. 
           CLOSE RVUFILE FILEIN.
           STOP RUN.
