       IDENTIFICATION DIVISION.
       PROGRAM-ID. RMC002.
       AUTHOR. SID WAITE.
       DATE-COMPILED. TODAY.
       ENVIRONMENT DIVISION.
       INPUT-OUTPUT SECTION.
       FILE-CONTROL.
           SELECT EMAILAUTHFILE ASSIGN TO "S30" ORGANIZATION IS INDEXED
           ACCESS MODE DYNAMIC RECORD KEY IS EA-KEY
           ALTERNATE RECORD KEY IS EA-MEDREC WITH DUPLICATES
           ALTERNATE RECORD KEY IS EA-NAME WITH DUPLICATES
           ALTERNATE RECORD KEY IS EA-EMAIL WITH DUPLICATES
           ALTERNATE RECORD KEY IS EA-AUTH WITH DUPLICATES
           ALTERNATE RECORD KEY IS EA-DATE-E WITH DUPLICATES
           LOCK MODE MANUAL.
           SELECT ENMAILAUTHFILE ASSIGN TO "S35" ORGANIZATION IS INDEXED
           ACCESS MODE DYNAMIC RECORD KEY IS EAN-KEY
           ALTERNATE RECORD KEY IS EAN-MEDREC WITH DUPLICATES
           ALTERNATE RECORD KEY IS EAN-NAME WITH DUPLICATES
           ALTERNATE RECORD KEY IS EAN-EMAIL WITH DUPLICATES
           ALTERNATE RECORD KEY IS EAN-AUTH WITH DUPLICATES
           ALTERNATE RECORD KEY IS EAN-DATE-E WITH DUPLICATES
           ALTERNATE RECORD KEY IS EAN-SSN WITH DUPLICATES
           LOCK MODE MANUAL.
            
       DATA DIVISION.
       FILE SECTION.
       FD  EMAILAUTHFILE.
       01  EMAILAUTHFILE01.
           02 EA-KEY PIC 9(6).
           02 EA-MEDREC PIC X(8).
           02 EA-NAME PIC X(24).
           02 EA-EMAIL PIC X(30).
           02 EA-AUTH PIC X(20).
           02 EA-DATE-E PIC X(8).
       FD  ENMAILAUTHFILE.
       01  ENMAILAUTHFILE01.
           02 EAN-KEY PIC 9(6).
           02 EAN-MEDREC PIC X(8).
           02 EAN-NAME PIC X(24).
           02 EAN-EMAIL PIC X(30).
           02 EAN-AUTH PIC X(20).
           02 EAN-DATE-E PIC X(8).
           02 EAN-SSN PIC X(9).
      *
       PROCEDURE DIVISION.
       0005-START.
           OPEN OUTPUT ENMAILAUTHFILE.
           OPEN INPUT EMAILAUTHFILE.
       P1.
           READ EMAILAUTHFILE NEXT AT END GO TO P99.
           MOVE EMAILAUTHFILE01 TO ENMAILAUTHFILE01
           MOVE SPACE TO EAN-SSN
           WRITE ENMAILAUTHFILE01
           GO TO P1.
       P99.
           CLOSE ENMAILAUTHFILE EMAILAUTHFILE.
           STOP RUN.
